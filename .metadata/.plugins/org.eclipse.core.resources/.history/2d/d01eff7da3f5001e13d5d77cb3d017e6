package basicpractical;

public class P002_Primenumber {
	public static void main(String[] args) {
		
		int num = 13;
		int flag = 0;
		for(int i=2;i<num;i++)
		{
			if(num%i==0)
			{
				flag = 1;
				break;
			}
		
		}
		
		if(flag==1)
		{
			System.out.println("not prime");
		}
		else
		{
			System.out.println("prime");
		}
		
	}
}
