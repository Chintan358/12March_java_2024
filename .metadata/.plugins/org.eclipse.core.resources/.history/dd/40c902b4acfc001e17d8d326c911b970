	public static void main(String[] args) {
		
		//string litral
		String str = "Hello";
		//String object
		String s = new String("Hello");
		
		String str1 =  str.concat("Java");
		
		System.out.println(str1);
		System.out.println(s);
		
		
		
		
		
		
		
		
		
		
		
	}
}
