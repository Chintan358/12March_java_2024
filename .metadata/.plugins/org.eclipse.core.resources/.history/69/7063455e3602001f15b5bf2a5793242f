package exception;

import java.util.Scanner;

public class FinallyDemo {
	public static void main(String[] args) {
		
		
		Scanner sc=null;
		try {
			
			 sc = new Scanner(System.in);
			int i = 10/0;
			
		} catch (Exception e) {
			e.printStackTrace();
		}
		finally
		{
			sc.close();
		}
		
	}
}
