package collection;

import java.util.HashMap;
import java.util.LinkedHashMap;

public class C010_Hashmap {
	public static void main(String[] args) {
		
		
		
		LinkedHashMap<String, String> map = new LinkedHashMap<String, String>();
		map.put("IN", "India");
		map.put("US", "usa");
		map.put("CAN", "canada");
		map.put("IN" ,"Iran");
		
		System.out.println(map);
		
		
		
		
	}
}
