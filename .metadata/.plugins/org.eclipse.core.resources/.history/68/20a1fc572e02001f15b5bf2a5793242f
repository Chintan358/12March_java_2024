package exception;

class Bank
{
	double balance;
	
	public void checkBalance()
	{
		System.out.println("current balanc eis : "+balance);
	}
	
	public void deposite(double amt)
	{
		balance = balance+amt;
	}
}



public class CustomeException {
	public static void main(String[] args) {
		
		
		Bank b = new Bank();
		b.checkBalance();
		b.deposite(5000);
		
	}
}
