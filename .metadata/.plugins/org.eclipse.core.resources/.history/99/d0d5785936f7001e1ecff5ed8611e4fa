package oops;

class A
{
	int id = 10;
	
	public void display()
	{
		System.out.println("Runing A class display");
	}
}

class B extends A
{
	int id = 50;
	public void show()
	{
		System.out.println(id);
		System.out.println("Runinng B class show");
	}
}

public class O002_InheritanceDemo {
	public static void main(String[] args) {
		
		
		B b  = new B();
		b.show();
		b.display();
		
	}
}
